swaWorksheet.Cells(l, l1).Value = .DurationText
l1 = l1 + 1

On Error Resume Next
swaWorksheet.Cells(l, l1).Value = .PercentComplete
l1 = l1 + 1

Dim feche As Date
'feche = "20/03/2018  6:00:00"

