               ' End If
                
            'unid = prosum.OutlineParent
            
            'Set prosum = pro.Tasks.UniqueID(unid)
            
           ' Loop Until lev = 0
            
        
        'End If
    
If texto <> "" Then
swaWorksheet.Cells(l, l1).Value = texto & vbNewLine & .Name
Else
swaWorksheet.Cells(l, l1).Value = .Name
End If

l1 = l1 + 1

swaWorksheet.Cells(l, l1).Value = CDate(.Created)
l1 = l1 + 1

