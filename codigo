l1 = l1 + 1
On Error Resume Next
swaWorksheet.Cells(l, l1).Value = (.RemainingOvertimeWork / 60) / pro.HoursPerDay
l1 = l1 + 1

On Error Resume Next
swaWorksheet.Cells(l, l1).Value = (.RegularWork / 60) / pro.HoursPerDay
l1 = l1 + 1

    On Error Resume Next
swaWorksheet.Cells(l, l1).Value = CDate(.BaselineStart)
l1 = l1 + 1
On Error Resume Next
